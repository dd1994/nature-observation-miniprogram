{
  "pages": [
    "pages/index/index",
    "pages/explore/index",
    "pages/mine/mine",
    "pages/observation-create/observation-create",
    "pages/observation-detail/observation-detail",
    "pages/observation-map/observation-map",
    "pages/taxon-detail/taxon-detail",
    "pages/index-search/index-search"
  ],
  "subPackages": [
    {
      "root": "mine-packages",
      "pages": [
        "pages/setting/setting",
        "pages/about/about",
        "pages/profile-setting/profile-setting",
        "pages/stat/stat",
        "pages/taxon-picker/taxon-picker"
      ]
    }
  ],
  "preloadRule": {
    "pages/mine/mine": {
      "network": "all",
      "packages": [
        "mine-packages"
      ]
    }
  },
  "tabBar": {
    "borderStyle": "@borderStyle",
    "backgroundColor": "@backgroundColor",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "@iconPath1",
        "selectedIconPath": "@selectedIconPath1"
      },
      {
        "pagePath": "pages/explore/index",
        "iconPath": "@iconPath2",
        "selectedIconPath": "@selectedIconPath2"
      },
      {
        "pagePath": "pages/mine/mine",
        "iconPath": "@iconPath3",
        "selectedIconPath": "@selectedIconPath3"
      }
    ]
  },
  "themeLocation": "theme.json",
  "window": {
    "navigationStyle": "default",
    "navigationBarBackgroundColor": "#FFF",
    "navigationBarTextStyle": "black"
  },
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "plugins": {
    "chooseLocation": {
      "version": "1.0.10",
      "provider": "wx76a9a06e5b4e693e"
    }
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将在创建记录时用于记录观察的地理位置"
    }
  }
}