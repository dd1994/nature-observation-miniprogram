{
  "pages": [
    "pages/index/index",
    "pages/explore/index",
    "pages/mine/mine",
    "pages/observation-create/observation-create",
    "pages/observation-detail/observation-detail",
    "pages/taxon-detail/taxon-detail",
    "pages/taxon-picker/taxon-picker"
  ],
  "subPackages": [
    {
      "root": "mine-packages",
      "pages": [
        "pages/setting/setting",
        "pages/about/about",
        "pages/profile-setting/profile-setting",
        "pages/stat/stat",
        "pages/ai-tools/ai-tools",
        "pages/user-home/user-home",
        "pages/notify/notify"
      ]
    },
    {
      "root": "index-packages",
      "pages": [
        "pages/index-filter/index-filter",
        "pages/index-search/index-search"
      ]
    }
  ],
  "preloadRule": {
    "pages/mine/mine": {
      "network": "all",
      "packages": [
        "mine-packages"
      ]
    }
  },
  "tabBar": {
    "borderStyle": "@borderStyle",
    "backgroundColor": "@backgroundColor",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "我的"
      },
      {
        "pagePath": "pages/explore/index",
        "text": "发现"
      },
      {
        "pagePath": "pages/mine/mine",
        "text": "设置"
      }
    ]
  },
  "themeLocation": "theme.json",
  "window": {
    "navigationStyle": "default",
    "navigationBarBackgroundColor": "#FFF",
    "navigationBarTextStyle": "black"
  },
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "2.31.1",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "plugins": {},
  "requiredPrivateInfos": [
    "chooseLocation"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将在创建记录时用于记录观察的地理位置"
    }
  }
}